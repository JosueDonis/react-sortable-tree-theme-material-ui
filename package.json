{
  "name": "react-sortable-tree-theme-material-ui",
  "version": "1.0.7",
  "description": "Material UI theme for react-sortable-tree",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production TARGET=umd webpack --bail",
    "build:demo": "npm run clean:demo && cross-env NODE_ENV=production TARGET=demo webpack --bail",
    "clean": "rimraf dist",
    "clean:demo": "rimraf build",
    "start": "cross-env NODE_ENV=development TARGET=development webpack-dev-server --inline --hot",
    "lint": "eslint .",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"**/*.{js,jsx,css,scss}\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "jest"
  },
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "release": {
    "branch": "next"
  },
  "publishConfig": {
    "tag": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grovertb/react-sortable-tree-theme-material-ui"
  },
  "homepage": "https://github.com/grovertb/react-sortable-tree-theme-material-ui",
  "bugs": "https://github.com/grovertb/react-sortable-tree-theme-material-ui/issues",
  "authors": [
    "Chris Fritz"
  ],
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@material-ui/styles": "^4.9.0",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.7.2",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dnd-scrollzone": "^5.0.0",
    "react-virtualized": "^9.21.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-sortable-tree": "^2.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-sort-keys-fix": "^1.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dnd-test-backend": "^10.0.2",
    "react-dnd-touch-backend": "^10.0.2",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "react-sortable-tree": "^2.7.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "keywords": [
    "react",
    "react-component"
  ]
}
